{
	"compilerOptions": {
		"lib": ["ES2023"],
		"module": "node16",
		"target": "ES2022",
		"rootDir": "./src",
		"outDir": "./dist",
		"removeComments": true,
		"newLine": "lf",
		"esModuleInterop": true,
		"forceConsistentCasingInFileNames": true,
		"strict": true,
		"skipLibCheck": true
	}
}
